<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <record model="ir.ui.view" id="vschool.list">
      <field name="name">vschool list</field>
      <field name="model">vschool.student</field>
      
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="arch" type="xml">
<xpath expr="//field[@name='phone']" position="after">
    <field name="grade" />
</xpath>
      </field>
    </record>
    

    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="vschool.action_window">
      <field name="name">vschool window</field>
      <field name="res_model">vschool.student</field>
      <field name="view_mode">tree,form</field>
    </record>
    

    <!-- server action to the one above -->
    <!--
    <record model="ir.actions.server" id="vschool.action_server">
      <field name="name">vschool server</field>
      <field name="model_id" ref="model_vschool_vschool"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>
    -->

    <!-- Top menu item -->
    
    <menuitem name="vschool" id="vschool.menu_root"/>
    
    <!-- menu categories -->
    
    <menuitem name="Menu 1" id="vschool.menu_1" parent="vschool.menu_root"/>
    <menuitem name="Menu 2" id="vschool.menu_2" parent="vschool.menu_root"/>
    
    <!-- actions -->
    
    <menuitem name="List" id="vschool.menu_1_list" parent="vschool.menu_1"
              action="vschool.action_window"/>
              <!--
    <menuitem name="Server to list" id="vschool" parent="vschool.menu_2"
              action="vschool.action_server"/>
               -->
  </data>
</odoo>